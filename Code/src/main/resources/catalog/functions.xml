<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="functions.xsl" ?>
<catalog>
    <category name="Math">
        <function name="(*)" signature="Float -> Float -> Float" blocktype="function_basic"></function>
        <function name="(+)" signature="Float -> Float -> Float" blocktype="function_basic"></function>
        <function name="(-)" signature="Float -> Float -> Float" blocktype="function_basic"></function>
        <function name="(/)" signature="Float -> Float -> Float" blocktype="function_basic"></function>
        <function name="(/=)" signature="Float -> Float -> Bool" blocktype="function_basic"></function>

        <function name="(^)" signature="Float -> Float -> Float" blocktype="function_basic"></function>

        <function name="(&lt;)" signature="Float -> Float -> Bool" blocktype="function_basic"></function>
        <function name="(==)" signature="Float -> Float -> Bool" blocktype="function_basic"></function>
        <function name="(>)" signature="Float -> Float -> Bool" blocktype="function_basic"></function>

        <function name="abs" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="acos" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="acosh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="asin" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="asinh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="atan" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="atanh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="ceiling" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="cos" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="cosh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="exp" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="floor" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="log" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="logBase" signature="Float -> Float -> Float" blocktype="function_basic"></function>
        <function name="negate" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="pred" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="recip" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="round" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="signum" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="sin" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="sinh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="sqrt" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="succ" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="tan" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="tanh" signature="Float -> Float" blocktype="function_basic"></function>
        <function name="truncate" signature="Float -> Float" blocktype="function_basic"></function>

        <function name="pi" signature="Float" blocktype="function_basic"></function>
    </category>
    <category name="Maps, folds and lists">
        <function name="head" signature="[a] -> a" blocktype="function_basic">
            Extract the first element of a list, which must be non-empty.
        </function>
    </category>
    <category name="Assorted">
        <function name="id" signature="a -> a" blocktype="function_basic">
            Identity function.
        </function>
        <function name="const" signature="a -> b -> a" blocktype="function_basic">
            Constant function.
        </function>
    </category>
    <category name="Special blocks">
        <function name="DisplayBlock" signature="" blocktype="display">
            A Block that can display its input.
        </function>
        <function name="ValueBlock" signature="" blocktype="value">
            A Block that has a constant value.
        </function>
    </category>
</catalog>
